{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "linebreak-style": "off",
        "react/jsx-indent": "off",
        "react/jsx-filename-extension": "off",
        "indent": "off",
        "comma-dangle": "off"
    }
}